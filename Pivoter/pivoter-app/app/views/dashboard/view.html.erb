<div class="container-fluid">
	<div class="row top15">

		<div class="col-md-12">
			<div class="row-fluid">

				<ul class="nav nav-pills">
				    <li class="active">
				    	<a href="#tracking" data-toggle="tab">Tracking <span class="badge"><%= @startup.pivots_count%></span></a>
				    </li>
			     	<li>
			     		<a href="#members" data-toggle="tab">Members <span class="badge"><%= @startup.members_count%></span></a>
				    </li>
				    <li data-toggle="tooltip" data-placement="bottom" title="Compare your results">
				    	<a href="#progress" data-toggle="tab">Progress</a>
				    </li>
				    <li data-toggle="tooltip" data-placement="bottom" title="Export your data">
				    	<a href="#reports" data-toggle="tab">Reports</a>
				    </li>
