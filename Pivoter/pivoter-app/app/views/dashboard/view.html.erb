				<div class="tab-content">
				  <div class="tab-pane active" id="tracking"><%= render :partial => 'tracking' %></div>
				  <div class="tab-pane" id="progress"><%= render :partial => 'progress' %></div>
				  <div class="tab-pane" id="reports"><%= render :partial => 'reports' %></div>
				  <div class="tab-pane" id="members"><%= render :partial => 'members' %></div>
                  <div class="tab-pane" id="entries"><%= render :partial => 'entries' %></div>
				</div>
			</div>
		</div>
